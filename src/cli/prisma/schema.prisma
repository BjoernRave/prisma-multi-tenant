datasource management {
  provider = ["sqlite","mysql","postgresql"]
  url      = env("PMT_MANAGEMENT_URL")
}

generator client {
  provider      = "prisma-client-js"
  output        = env("PMT_OUTPUT")
  binaryTargets = ["native"]
}

model Tenant {
  id       String @default(cuid()) @id
  name     String @unique
  provider String
  url      String
}